<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
    <Dialog title="{i18n>TITLE_CHARVALUE_HELP}" verticalScrolling="false">
        <content>
            <List itemPress="_onCharValSelected" mode="SingleSelectMaster" items="{/}">
                <items>
                    <StandardListItem type="Active" title="{Atwrt}" info="{Atwtb}"/>
                </items>
            </List>
        </content>
        <buttons>
            <Button text="{i18n>CLOSE}" press="onDialogClose"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>